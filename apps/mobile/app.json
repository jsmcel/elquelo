{
  "expo": {
    "name": "ELQUELO Despedidas",
    "slug": "elquelo-despedidas",
    "scheme": "elquelo",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "../..//public/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "../..//public/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "../..//public/logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "plugins": [
      "expo-router",
      ["expo-secure-store"],
      ["expo-notifications"]
    ]
  }
}
